        if ((moveDir &gt;0 &amp;&amp; moveDir &lt; 25 /*+ 20*/ ) or (moveDir &lt; 359 &amp;&amp; moveDir &gt; 335 /*- 20*/) ) //Right
        {
          /*keyboard_key_press(vk_right);
