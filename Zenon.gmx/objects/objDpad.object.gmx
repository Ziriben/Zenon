                            image_index=6;
          
                           // show_debug_message("↙↙↙");
                           }
                            
else if keyboard_check(vk_left) {
                            image_index=7;
                           // show_debug_message("◄◄◄");
                           }
                            
if (keyboard_check(vk_left) &amp;&amp; keyboard_check(vk_up)) {
                            image_index=8;
                           // show_debug_message("↖↖↖");
                           }

